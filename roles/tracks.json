{
    "name": "tracks",
    "description": "Tracks Application role",
    "chef_type": "role",
    "json_class": "Chef::Role",
    "default_attributes": {
      "anevis-base":{
        "ports": ["22", "3306"]
      },
      "rbenv":{
        "user_installs":[
          {
            "user":"tracks",
            "rubies":["2.4.0"],
            "global":"2.4.0",
            "gems":{
              "2.4.0" :[
                { "name" : "json" },
                { "name" : "bundler" }
              ]
            }
          }
        ]
      }
    },
    "override_attributes": {
    },
    "run_list": [
        "role[base]",
        "recipe[ruby_build]",
        "recipe[ruby_rbenv]",
        "recipe[medi-tracks]"
    ]
}
